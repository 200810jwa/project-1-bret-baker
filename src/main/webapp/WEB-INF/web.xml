<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_3.xsd"
	version="2.3">

	<display-name>project-1-bret-baker</display-name>

	<welcome-file-list>
		<welcome-file>/static/index.html</welcome-file>
	</welcome-file-list>

	<servlet>
		<description></description>
		<display-name>LoginServlet</display-name>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>com.revpro1.web.LoginServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<servlet>
		<description></description>
		<display-name>HelloServlet</display-name>
		<servlet-name>HelloServlet</servlet-name>
		<servlet-class>com.revpro1.web.HelloServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>HelloServlet</servlet-name>
		<url-pattern>/hello/*</url-pattern>
	</servlet-mapping>

	<servlet>
		<description></description>
		<display-name>ReimbServlet</display-name>
		<servlet-name>ReimbServlet</servlet-name>
		<servlet-class>com.revpro1.web.ReimbServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ReimbServlet</servlet-name>
		<url-pattern>/reimbs</url-pattern>
	</servlet-mapping>

</web-app>